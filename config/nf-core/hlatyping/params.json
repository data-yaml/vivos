{
    "input": "{{ samplesheet }}",
    "outdir": "quilt+{{ bucket }}#package={{ package }}",
    "solver": "glpk",
    "enumerations": 1,
    "beta": 0.009,
    "igenomes_base": "s3://ngi-igenomes/igenomes",
    "igenomes_ignore": false,
    "custom_config_version": "master",
    "custom_config_base": "https://raw.githubusercontent.com/nf-core/configs/master",
    "config_profile_name": "Full test profile",
    "config_profile_description": "Full test dataset (fastq and bam files) to check pipeline function",
    "max_cpus": 16,
    "max_memory": "128.GB",
    "max_time": "240.h",
    "help": false,
    "publish_dir_mode": "copy",
    "plaintext_email": false,
    "max_multiqc_email_size": "25.MB",
    "monochrome_logs": false,
    "multiqc_config": "${projectDir}/assets/multiqc_optitype_config.yml",
    "multiqc_logo": "${projectDir}/assets/nf-core-hlatyping_logo_light.png",
    "tracedir": "${params.outdir}/pipeline_info",
    "validate_params": true,
    "schema_ignore_params": "benchling",
    "show_hidden_params": false
}
